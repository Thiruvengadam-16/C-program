#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    scanf("%d",&a);
    if(a>=0 && a<=9){
    switch(a){
        case(0):printf("Integer is 0 and String is Zero");
        break;
        case(1):printf("Integer is 1 and String is One");
        break;
        case(2):printf("Integer is 2 and String is Two");
        break;
        case(3):printf("Integer is 3 and String is Three");
        break;
        case(4):printf("Integer is 4 and String is Four");
        break;
        case(5):printf("Integer is 5 and String is Five");
        break;
        case(6):printf("Integer is 6 and String is Six");
        break;
        case(7):printf("Integer is 7 and String is Seven");
        break;
        case(8):printf("Integer is 8 and String is Eight");
        break;
        case(9):printf("Integer is 9 and String is Nine");
        break;
        
    }}
    else 
        printf("Invalid Input");
        
    return 0;
}
