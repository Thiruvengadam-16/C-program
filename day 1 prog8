#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    float a,b,c,d;
    scanf("%f",&a);
    scanf("%f",&b);
    scanf("%f",&c);
    scanf("%f",&d);
    printf("%d\n",(int)a);
    printf("%d\n",(int)ceil(a));
    printf("%d\n",(int)sqrt(b));
    printf("%d",(int)pow(c,d));
    return 0;
}
