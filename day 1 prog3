#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    scanf("%d",&a);
    
    float b;
    scanf("%f",&b);
    
    char c[100],d[100],e[100];
    scanf("%s\n",&c);
    scanf("%s\n",d);
    scanf("%[^\n]s",e);
    printf("Integer is %d\n",a);
    printf("Float is %.2f\n",b);
    printf("Character is %s\n",c);
    printf("Strings are %s and %s",d,e);
    return 0;
}
